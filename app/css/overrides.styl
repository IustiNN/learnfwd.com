@media (max-width: 991px)
  .dropdown-menu > li > a
    color: white
  .navbar
    background-color: mm-dark-blue
  .navbar-header
    float: none
  .navbar-toggle
    display: block
  .navbar-collapse
    border-top: 1px solid transparent
    box-shadow: inset 0 1px 0 rgba(255,255,255,0.1)
    background-color: mm-dark-blue
  .navbar-collapse.collapse
    display: none!important
    background-color: mm-dark-blue
  .navbar-nav
    float: none!important
    margin: 7.5px -15px
  .navbar-nav>li
    float: none
  .navbar-nav>li>a
    padding-top: 10px
    padding-bottom: 10px
    color: white !important
  .navbar-collapse.collapse.in
    display: block!important
    background-color: mm-dark-blue
  .collapsing
    overflow: hidden!important
    background-color: mm-dark-blue
  .navbar-right
    float: none
  .dropdown-menu
    position: initial
    top: 0
    float: none
    margin: 0
    background-color: inherit
    border none
    box-shadow: none
@media (min-width: 991px) {
  .navbar-right .dropdown-menu {
    right: 0;
    left: auto;
  }
  .navbar-right .dropdown-menu-left {
    right: auto;
    left: 0;
  }
}
