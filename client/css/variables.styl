lfwd-serif ?= 'Roboto Slab', 'Times New Roman', Times, Georgia, serif;
lfwd-sans ?= 'Roboto', 'Helvetica Neue', Helvetica, Arial, sans-serif;
lfwd-cursive ?= 'Unica One', sans-serif;

lfwd-orange ?= #FAAF3B
lfwd-pumpkin ?= #FAAF3B
lfwd-asphalt ?= #34495e
lfwd-pomgranate ?= #c0392b
lfwd-green ?= #8BC540

h1, h2, h3, h4, h5, div, li, ul, ol, .sans
  font-family: lfwd-sans
  font-weight: 300

p, .serif
  font-family: lfwd-serif

.cursive
  font-family: lfwd-cursive

.thin
  font-weight: 100

.normal
  font-weight: 400
